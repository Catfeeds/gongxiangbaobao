/* pages/userTwo/userTwo.wxss */


.Two-bg{
  background-color: white;
}
.user-photo-bgc{
  height: 330rpx;
  width: 750rpx;
  position: relative;

}
.user-info-box{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
    display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.user-photo{
  height: 130rpx;
  width: 130rpx;
  border-radius: 50%;
}
.user-name-text{
  font-size: 34rpx;
  color: #FFFFFF;
}
.user-id{
  font-size: 26rpx;
  color: #FFFFFF;
}
.middle-info{
  height: 238rpx;
  border-bottom: 2rpx solid RGB(230,230,230); 
  font-size: 28rpx;
  color: #222222 ;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  background-color: white;
  margin-bottom: 38rpx;
  font-weight: 700;
}
.title-photo{
  height: 120rpx;
  width: 120rpx;
  background-color: #F7E9CF;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.middle-info image{
  height: 58rpx;
  width: 68rpx;
}
.title-out-box{
  height: 238rpx;
    display: flex;
    flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.bottom-btn{
  height: 90rpx;
  border-top: 2rpx solid RGB(230,230,230);
  line-height: 90rpx;
  padding:0 66rpx;
  font-size: 30rpx;
  color: #222222;
  font-weight: 700;
  background-color: white;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.last-btn{
  border-bottom:  2rpx solid RGB(230,230,230);
}
.my-iocn{
  font-family: "Fineix";
  font-size: 24rpx;
  color: RGB(204,204,204);
font-weight: 700;
}
button{
  border:none !important;
  font-size: 30rpx;
  background-color: white;
  margin: 0;
  padding: 0;
}
button::after {
  border: none;
}