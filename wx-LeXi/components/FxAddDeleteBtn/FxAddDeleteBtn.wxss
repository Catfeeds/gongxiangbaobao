.changBox {
  height: 100%;
  width: 100%;
  border: 1rpx solid #d5d5d6;
  border-radius: 10rpx;
  display: flex;
}

.changBox view {
  flex: 1;
  color: #949EA6;
}

.reduce {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}

.addicon {
  font-size: 44rpx;
}

.goodsNumber {
  border-left: 2rpx solid #e9e9e9;
  border-right: 2rpx solid #e9e9e9;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #333 !important;
  font-size: 24rpx;
}
