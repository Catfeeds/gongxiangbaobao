/* components/FxProductComponent/FxProductComponent.wxss */
.photoBox{
  width: 100%;
  padding-bottom: 100%;
   height: 0rpx; 
  background-color: red;
  position: relative;
  border-radius: 10rpx; 

}
.photoBox image{
  background-color: red;
  height: 100%;
  width: 100%;
}
.sellInfo{
  position: absolute;
  width: 62rpx;
  height: 36rpx;
  background-color: #858583;
  font-size: 20rpx;
  border-radius: 5rpx;
  color: white;
  line-height: 36rpx;
  text-align: center;
  top: 15rpx;
  left: 15rpx;
}
.material-quality>view{
display: inline-block;
}
.logistics{
  height: 26rpx;
  width: 42rpx;
  border-radius: 5rpx;
  background-color: #D4AF86;
  color: white;
  font-size: 20rpx;
  line-height: 26rpx;
  text-align: center;
  margin-right: 6rpx;
}
.material-quality-text{
  font-size: 24rpx;
}
.material-quality{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.oldPrice,
.nowPrice{
  font-size: 24rpx;
  margin-right: 10rpx;

}
