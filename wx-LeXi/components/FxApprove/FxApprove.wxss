/* components/FxApprove/FxApprove.wxss */
.is-approve {
  height: 34rpx;
  width: 120rpx;
  border-radius: 30rpx;
  border: 2rpx solid #f2c768;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  font-size: 24rpx;
  color: #f2c768;
}

.is-approve>image {
  height: 30rpx;
  width: 30rpx;
  border-radius: 50%;
  background-color: red;
}