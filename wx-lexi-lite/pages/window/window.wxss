/* 头图 */

.fx-window {
  border: none;
}

.fx-window__images {
  position: relative;
}

.window-title__name {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 248rpx;
  background: url("https://static.moebeast.com/static/img/maskx766.png");
  background-size: 100% 100%;
  z-index: 2;
}

.window-title__text {
  position: absolute;
  bottom: 40rpx;
  padding: 0 30rpx;
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}
