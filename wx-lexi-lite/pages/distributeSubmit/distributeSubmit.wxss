/* pages/distributeSubmit/distributeSubmit.wxss */
.distribute-form {
  padding: 0rpx 40rpx;
  border-bottom: 1rpx solid #e9e9e9;
  margin-bottom: 20rpx;
  background-color: #fff;
}
.distribute-form .form-title {
  font-size: 40rpx;
  color: #333;
}
.distribute-form .input-text {
  color: #333;
  font-size: 28rpx;
  padding: 40rpx 0rpx;
  line-height: 1.5;
  width: 100%;
}
.distribute-form .form-holder {
  color: #B2B2B2;
  font-size: 28rpx;
  line-height: 40rpx;
}
.distribute-products {
  background-color: #fff;
}
.distribute-action {
  padding: 60rpx 40rpx;
  border-top: 1rpx solid #e9e9e9;
}