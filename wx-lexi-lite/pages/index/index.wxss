.swiper-photo{
  background-color: white;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.swiper-photo .hidding {
  transform: scale(0);
  /* visibility: hidden; */
  transition: all 0.2s ease-in 0.1s;
}

.swiper-photo swiper {
width: 100%;
height: 400rpx;
margin: 0 auto;
}

.swiper-photo swiper-item {
background: #fff;
padding-top: 30rpx;
overflow: hidden !important;
}

/* .imgpic3{

} */

.swiper-photo image {
width: 86%;
height: 280rpx;
border-radius: 8rpx;
margin: 0 auto;
display: block;

}

.swiper-photo image.active {
transform: scale(2);
transition: all 0.2s ease-in 0s;
}

.swiper-photo image.active .prompt-textwrap {
display: block;
}

.swiper-photo .slogan {
color: #d4d2d3;
font-size: 47rpx;
position: absolute;
bottom: -90rpx;
left: 0;
display: none;
}

.swiper-photo image.active .slogan {
display: block;
}

