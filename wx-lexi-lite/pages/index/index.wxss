}

/* .imgpic3{

} */

.swiper-photo .slide-item__box {
  width: 86%;
  height: 280rpx;
  border-radius: 8rpx;
  margin: 0 auto;
  display: block;
  overflow: hidden;
}

swiper-photo .slide-item__box image {
  height: 100% !important;
  width: 100%;
}

.swiper-photo .slide-item__box.active {
  /* transform: scaleX(1),scaleY(1.4);
  transition: all 0.2s ease-in 0s; */
  width: 73%;
  height: 280rpx;
  border-radius: 8rpx;
  margin: 0 auto;
  display: block;
  overflow: hidden;
  animation: myfirst 0.3s;
  animation-fill-mode: forwards;
}

.swiper-photo .slide-item__box image {
  height: 100%;
  border-radius: 8rpx;
}

@keyframes myfirst {
  from {
    width: 86%;
    height: 280rpx;
  }

  to {
    width: 100%;
    height: 100%;
    margin-left: 20rpx;
  }
}

.swiper-photo .slide-item__box .active .prompt-textwrap {
  display: block;
}

.swiper-photo .slogan {
  color: #d4d2d3;
  font-size: 47rpx;
  position: absolute;
  bottom: -90rpx;
  left: 0;
  display: none;
}

.swiper-photo .slide-item__box.active .slogan {
