      <fx-goods-list></fx-goods-list>
    </view>
  </view>

  <!-- 冰冰乐 -->
  <view class='goods-name-info-list'>
    <view class='rander-coffee'>
      <view class='rander-coffee-l'>
        <image src='../../images/frappuccino@3xone.png'></image>
        <view class='coffe-text'>
          <view>冰冰乐</view>
